                    </li>
                    <li  {% if onglet == 'Personne' %}class="current"{% endif %}>
                        <a href="{{path('personne')}}" data-description="Person">Personne</a>
                        <ul>
                            <li>
                                <a href="{{path('personne_find')}}">Recherche par nom</a>
                            </li>
                        </ul>
                    </li>
                    <li  {% if onglet == 'Soin' %}class="current"{% endif %}>
                        <a href="{{path('soin')}}" data-description="Care">Soin</a>
                    </li>
                    <li  {% if onglet == 'Motif Sortie' %}class="current"{% endif %}>
                        <a href="{{path('motifsortie')}}" data-description="Care">Motif Sortie</a>
                    </li>
 
 
 
                </ul>
 
            </nav><!-- end #main-nav -->
 

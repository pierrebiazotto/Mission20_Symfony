<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="demandeentente" pattern="/">
        <default key="_controller">GSBPatientsBundle:Demandeentente:index</default>
    </route>

    <route id="demandeentente_show" pattern="/{id}/show">
        <default key="_controller">GSBPatientsBundle:Demandeentente:show</default>
    </route>

    <route id="demandeentente_new" pattern="/new">
        <default key="_controller">GSBPatientsBundle:Demandeentente:new</default>
    </route>

    <route id="demandeentente_create" pattern="/create">
        <default key="_controller">GSBPatientsBundle:Demandeentente:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="demandeentente_edit" pattern="/{id}/edit">
        <default key="_controller">GSBPatientsBundle:Demandeentente:edit</default>
    </route>

    <route id="demandeentente_update" pattern="/{id}/update">
        <default key="_controller">GSBPatientsBundle:Demandeentente:update</default>
        <requirement key="_method">post|put</requirement>
    </route>

    <route id="demandeentente_delete" pattern="/{id}/delete">
        <default key="_controller">GSBPatientsBundle:Demandeentente:delete</default>
        <requirement key="_method">post|delete</requirement>
    </route>

</routes>

<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="medecin" pattern="/">
        <default key="_controller">GSBPatientsBundle:Medecin:index</default>
    </route>

    <route id="medecin_show" pattern="/{id}/show">
        <default key="_controller">GSBPatientsBundle:Medecin:show</default>
    </route>

    <route id="medecin_new" pattern="/new">
        <default key="_controller">GSBPatientsBundle:Medecin:new</default>
    </route>

    <route id="medecin_create" pattern="/create">
        <default key="_controller">GSBPatientsBundle:Medecin:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="medecin_edit" pattern="/{id}/edit">
        <default key="_controller">GSBPatientsBundle:Medecin:edit</default>
    </route>

    <route id="medecin_update" pattern="/{id}/update">
        <default key="_controller">GSBPatientsBundle:Medecin:update</default>
        <requirement key="_method">post|put</requirement>
    </route>

    <route id="medecin_delete" pattern="/{id}/delete">
        <default key="_controller">GSBPatientsBundle:Medecin:delete</default>
        <requirement key="_method">post|delete</requirement>
    </route>

</routes>

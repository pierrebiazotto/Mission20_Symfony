<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="motifsortie" pattern="/">
        <default key="_controller">GSBPatientsBundle:Motifsortie:index</default>
    </route>

    <route id="motifsortie_show" pattern="/{id}/show">
        <default key="_controller">GSBPatientsBundle:Motifsortie:show</default>
    </route>

    <route id="motifsortie_new" pattern="/new">
        <default key="_controller">GSBPatientsBundle:Motifsortie:new</default>
    </route>

    <route id="motifsortie_create" pattern="/create">
        <default key="_controller">GSBPatientsBundle:Motifsortie:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="motifsortie_edit" pattern="/{id}/edit">
        <default key="_controller">GSBPatientsBundle:Motifsortie:edit</default>
    </route>

    <route id="motifsortie_update" pattern="/{id}/update">
        <default key="_controller">GSBPatientsBundle:Motifsortie:update</default>
        <requirement key="_method">post|put</requirement>
    </route>

    <route id="motifsortie_delete" pattern="/{id}/delete">
        <default key="_controller">GSBPatientsBundle:Motifsortie:delete</default>
        <requirement key="_method">post|delete</requirement>
    </route>

</routes>

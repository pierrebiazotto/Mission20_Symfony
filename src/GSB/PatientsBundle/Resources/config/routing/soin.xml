<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="soin" pattern="/">
        <default key="_controller">GSBPatientsBundle:Soin:index</default>
    </route>

    <route id="soin_showAll" pattern="/showAll">
        <default key="_controller">GSBPatientsBundle:Soin:showAll</default>
    </route>

    <route id="soin_show" pattern="/{id}/show">
        <default key="_controller">GSBPatientsBundle:Soin:show</default>
    </route>

    <route id="soin_new" pattern="/new">
        <default key="_controller">GSBPatientsBundle:Soin:new</default>
    </route>

    <route id="soin_create" pattern="/create">
        <default key="_controller">GSBPatientsBundle:Soin:create</default>
    </route>

    <route id="soin_edit" pattern="/{id}/edit">
        <default key="_controller">GSBPatientsBundle:Soin:edit</default>
    </route>

    <route id="soin_update" pattern="/{id}/update">
        <default key="_controller">GSBPatientsBundle:Soin:update</default>
        <requirement key="_method">post|put</requirement>
    </route>

    <route id="soin_delete" pattern="/{id}/delete">
        <default key="_controller">GSBPatientsBundle:Soin:delete</default>
        <requirement key="_method">post|delete</requirement>
    </route>

</routes>

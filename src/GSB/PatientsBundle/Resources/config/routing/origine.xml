<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="origine" pattern="/">
        <default key="_controller">GSBPatientsBundle:Origine:index</default>
    </route>

    <route id="origine_show" pattern="/{id}/show">
        <default key="_controller">GSBPatientsBundle:Origine:show</default>
    </route>

    <route id="origine_new" pattern="/new">
        <default key="_controller">GSBPatientsBundle:Origine:new</default>
    </route>

    <route id="origine_create" pattern="/create">
        <default key="_controller">GSBPatientsBundle:Origine:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="origine_edit" pattern="/{id}/edit">
        <default key="_controller">GSBPatientsBundle:Origine:edit</default>
    </route>

    <route id="origine_update" pattern="/{id}/update">
        <default key="_controller">GSBPatientsBundle:Origine:update</default>
        <requirement key="_method">post|put</requirement>
    </route>

    <route id="origine_delete" pattern="/{id}/delete">
        <default key="_controller">GSBPatientsBundle:Origine:delete</default>
        <requirement key="_method">post|delete</requirement>
    </route>

</routes>

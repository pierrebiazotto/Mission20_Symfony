<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="soinbase" pattern="/">
        <default key="_controller">GSBPatientsBundle:Soinbase:index</default>
    </route>

    <route id="soinbase_show" pattern="/{id}/show">
        <default key="_controller">GSBPatientsBundle:Soinbase:show</default>
    </route>

    <route id="soinbase_new" pattern="/new">
        <default key="_controller">GSBPatientsBundle:Soinbase:new</default>
    </route>

    <route id="soinbase_create" pattern="/create">
        <default key="_controller">GSBPatientsBundle:Soinbase:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="soinbase_edit" pattern="/{id}/edit">
        <default key="_controller">GSBPatientsBundle:Soinbase:edit</default>
    </route>

    <route id="soinbase_update" pattern="/{id}/update">
        <default key="_controller">GSBPatientsBundle:Soinbase:update</default>
        <requirement key="_method">post|put</requirement>
    </route>

    <route id="soinbase_delete" pattern="/{id}/delete">
        <default key="_controller">GSBPatientsBundle:Soinbase:delete</default>
        <requirement key="_method">post|delete</requirement>
    </route>

</routes>
